        Set Preview3(i).PicScrollerVertical_DISABLED = SelectedControls(0).PicScrollerVertical_DISABLED
        'Set preview property scroller horizontal
        Set Preview3(i).PicScrollerHorizontal_UP = SelectedControls(0).PicScrollerHorizontal_UP
        Set Preview3(i).PicScrollerHorizontal_DOWN = SelectedControls(0).PicScrollerHorizontal_DOWN
        Set Preview3(i).PicScrollerHorizontal_HOOVER = SelectedControls(0).PicScrollerHorizontal_HOOVER
        Set Preview3(i).PicScrollerHorizontal_DISABLED = SelectedControls(0).PicScrollerHorizontal_DISABLED
    Next i
    ' Set maximum dimensions for pictureboxes
    SetMaxDim
    Loading = False
End Sub

'================================================================
' Set maximum dimensions for pictureboxes
'================================================================
Private Sub SetMaxDim()
    ' Buttons and scroller
    For i = 0 To 3
        btnTop(i).AutoSize = True
        btnTop(i).AutoSize = True
        BtnBottom(i).AutoSize = True
        BtnBottom(i).AutoSize = True
        btnLeft(i).AutoSize = True
        btnLeft(i).AutoSize = True
        btnRight(i).AutoSize = True
        btnRight(i).AutoSize = True
        ScrlVert(i).AutoSize = True
        scrlHor(i).AutoSize = True
    Next i
    For i = 0 To 3
        If btnTop(i).Height > 735 Then btnTop(i).Height = 735
        If btnTop(i).Width > 735 Then btnTop(i).Width = 735
        If BtnBottom(i).Height > 735 Then BtnBottom(i).Height = 735
        If BtnBottom(i).Width > 735 Then BtnBottom(i).Width = 735
        If btnLeft(i).Height > 735 Then btnLeft(i).Height = 735
        If btnLeft(i).Width > 735 Then btnLeft(i).Width = 735
        If btnRight(i).Height > 735 Then btnRight(i).Height = 735
        If btnRight(i).Width > 735 Then btnRight(i).Width = 735
        If ScrlVert(i).Width > 735 Then ScrlVert(i).Width = 735
        If scrlHor(i).Width > 735 Then scrlHor(i).Width = 735
    Next i
    For i = 0 To 3
        btnTop(i).AutoSize = False
        btnTop(i).AutoSize = False
        BtnBottom(i).AutoSize = False
        BtnBottom(i).AutoSize = False
        btnLeft(i).AutoSize = False
        btnLeft(i).AutoSize = False
        btnRight(i).AutoSize = False
        btnRight(i).AutoSize = False
        ScrlVert(i).AutoSize = False
        scrlHor(i).AutoSize = False
    Next i
    ' Barback
    For i = 0 To 3
        Back(i).AutoSize = True
    Next i
    If Back(0).Width > 1335 Then Back(0).Width = 1335
    If Back(2).Width > 1335 Then Back(2).Width = 1335
    If Back(0).Height > 2055 Then Back(0).Height = 2055
    If Back(2).Height > 2055 Then Back(2).Height = 2055
    If Back(1).Width > 7215 Then Back(1).Width = 7215
    If Back(3).Width > 7215 Then Back(3).Width = 7215
    If Back(1).Height > 2055 Then Back(1).Height = 2055
    If Back(3).Height > 2055 Then Back(3).Height = 2055
    For i = 0 To 3
        Back(i).AutoSize = False
    Next i
End Sub

