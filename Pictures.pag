'=====================================================
' Save changed settings
'=====================================================
Private Sub SaveTheSettings()
    ' Saves the last selected path and filters with savesetting of usercontrol
    LastFilterstring = ""
    For i = 0 To 4
        If chkFilter(i) = vbChecked Then
            LastFilterstring = LastFilterstring & chkFilter(i).Caption & ";"
        End If
    Next i
    If Trim$(Dir1.Path) <> "" Then SaveSetting "DMSCROLLBAR", "VALUES", "LASTPATH", Dir1.Path
    If Trim$(LastFilterstring) <> "" Then SaveSetting "DMSCROLLBAR", "VALUES", "FILTER", Left$(LastFilterstring, Len(LastFilterstring) - 1)
End Sub

