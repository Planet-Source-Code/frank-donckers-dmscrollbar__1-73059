    Set NewPic = ScrlVert(Index)
    fraSelectPicure.Visible = True
End Sub

'=====================================================
' Select/deselect filefilters
'=====================================================
Private Sub Command1_Click()
    If Command1.Caption = "Select all" Then
        For i = 0 To 4
            LastFilter(i) = chkFilter(i).Value
            chkFilter(i).Value = vbChecked
        Next i
        Command1.Caption = "Reset all"
    Else
        For i = 0 To 4
            chkFilter(i).Value = LastFilter(i)
        Next i
        Command1.Caption = "Select all"
    End If
End Sub

