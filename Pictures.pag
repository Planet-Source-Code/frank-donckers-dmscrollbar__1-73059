'================================================================
' Get settings for last used path and filters
'================================================================
Private Sub GetTheSettings()
    ' Gets the last selected path and filters with Getsetting of usercontrol
    ' Last selected path
    LastPath = Trim$(GetSetting("DMSCROLLBAR", "VALUES", "LASTPATH", ""))
    LastFilterstring = Trim$(GetSetting("DMSCROLLBAR", "VALUES", "FILTER", ""))
    If LastPath <> "" Then
        Drive1.Drive = LastPath
        Dir1.Path = LastPath
        File1.Path = LastPath
    End If
    ' Last selected filters
    For i = 0 To 4
        chkFilter(i).Value = vbUnchecked
    Next i
    If LastFilterstring <> "" Then
        File1.Pattern = LastFilterstring
        Dim Pos As Integer
        Pos = InStr(1, LastFilterstring, "*.bmp", vbTextCompare)
        If Pos > 0 Then chkFilter(0).Value = vbChecked
        Pos = InStr(1, LastFilterstring, "*.jpg", vbTextCompare)
        If Pos > 0 Then chkFilter(1).Value = vbChecked
        Pos = InStr(1, LastFilterstring, "*.gif", vbTextCompare)
        If Pos > 0 Then chkFilter(2).Value = vbChecked
        Pos = InStr(1, LastFilterstring, "*.tif", vbTextCompare)
        If Pos > 0 Then chkFilter(3).Value = vbChecked
        Pos = InStr(1, LastFilterstring, "*.png", vbTextCompare)
        If Pos > 0 Then chkFilter(4).Value = vbChecked
        Dim AllSelected As Boolean
        AllSelected = True
        For i = 0 To 4
            If chkFilter(i).Value = vbUnchecked Then
                AllSelected = False
                Exit For
            End If
        Next i
        If AllSelected = True Then Command1.Caption = "Reset all"
    End If
End Sub

