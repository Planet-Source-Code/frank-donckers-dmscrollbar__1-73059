'=====================================================
' Enable/disable frames
'=====================================================
Private Sub Disable_EnableFrames(IsEnabled As Boolean)
    fraBar.Enabled = IsEnabled
    FraButtons.Enabled = IsEnabled
    fraDisabled.Enabled = IsEnabled
    fraScroll.Enabled = IsEnabled
    On Error GoTo InvalidColor
    PicColor.BackColor = ColorPic.BackColor
    Dim Hue As Integer
    Dim Sat As Integer
    Dim Lum As Integer
    rgb_get PicColor.BackColor
    RGBtoHSL txtRed.Text, txtGreen.Text, txtBlue.Text, Hue, Sat, Lum
    txtHue.Text = Hue
    txtSat.Text = Sat
    txtLum.Text = Lum
    HueScroll.Value = txtHue.Text
    SatScroll.Value = txtSat.Text
    LumScroll.Value = txtLum.Text
    DrawShadeAndLight PicColor.BackColor, &H0
    GoTo ValidColor
InvalidColor:
    PicColor.BackColor = &H0
ValidColor:
End Sub

